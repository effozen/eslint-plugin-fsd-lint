    "eslint": "^9.21.0",
    "vitest": "^3.0.7"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/effozen/eslint-plugin-fsd-lint.git"
  },
  "bugs": {
    "url": "https://github.com/effozen/eslint-plugin-fsd-lint/issues"
  },
  "homepage": "https://github.com/effozen/eslint-plugin-fsd-lint#readme"
